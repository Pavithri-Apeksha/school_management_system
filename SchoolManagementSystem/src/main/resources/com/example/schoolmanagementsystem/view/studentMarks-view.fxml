                   <TableColumn fx:id="studentIdCol" text="Student ID" />
                   <TableColumn fx:id="nameCol" text="Name" />
                   <TableColumn fx:id="totalCol" text="Total" />
                   <TableColumn fx:id="averageCol" text="Average" />
